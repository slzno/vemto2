{
    "name": "vemto",
    "productName": "Vemto",
    "version": "2.1.4",
    "description": "The ultimate studio for Laravel",
    "main": "build/src/main/main.js",
    "scripts": {
        "dev": "node scripts/dev-server.js",
        "dev:fast": "cross-env VEMTO_NO_CHECKERS=1 VEMTO_FAST_MODE=1 node scripts/dev-server.js",
        "dev:record": "cross-env VEMTO_RECORDING=1 VEMTO_NO_CHECKERS=1 VEMTO_FAST_MODE=1 node scripts/dev-server.js",
        "no-menu": "cross-env VEMTO_HIDE_MENU=1 node scripts/dev-server.js",
        "build:vite": "node scripts/prepare-build.js && electron-vite build",
        "build:local": "node scripts/no-sign-config.js && node scripts/prepare-build.js && electron-vite build && node scripts/post-build.js && electron-builder --config electron-builder-no-sign.json",
        "build": "node scripts/prepare-build.js && electron-vite build && node scripts/post-build.js && electron-builder --publish always",
        "build:win": "node scripts/build.js && electron-builder --win",
        "build:mac": "node scripts/build.js && electron-builder --mac",
        "build:linux": "node scripts/prepare-build.js && electron-vite build && node scripts/post-build.js && electron-builder --linux --x64 --publish always",
        "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "prettier": "npx prettier . --write",
        "php:compile": "php ./src/php/compile.php",
        "php:test": "cross-env VEMTO_DEBUG=1 php ./src/php/tests/vendor/bin/phpunit ./src/php/tests/src --verbose",
        "php:test-output": "cross-env VEMTO_DEBUG=1 php ./src/php/output.php",
        "php:test-schema": "cross-env VEMTO_DEBUG=1 php ./src/php/output.php schema-reader",
        "php:test-db-reader": "cross-env VEMTO_DEBUG=1 php ./src/php/output.php db-reader",
        "php:test-merger": "cross-env VEMTO_DEBUG=1 php ./src/php/output-merger.php",
        "test:mock": "yarn php:test-output && php ./mock.php",
        "test": "jest --runInBand",
        "test:watch": "jest --watch",
        "test:features": "npm run test -- --selectProjects test-features --",
        "test:templates": "npm run test -- --selectProjects test-templates --",
        "test:unit": "npm run test -- --selectProjects test-unit --"
    },
    "homepage": "https://vemto.app",
    "repository": {
        "type": "git",
        "url": "https://github.com/TiagoSilvaPereira/vemto2-releases"
    },
    "author": {
        "name": "King of Code",
        "url": "https://kingofcode.com.br"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.18.6",
        "@electron/notarize": "^2.3.0",
        "@jest/globals": "^29.3.1",
        "@tailwindcss/forms": "^0.5.3",
        "@vitejs/plugin-vue": "^3.0.3",
        "@vue/cli": "^5.0.1",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "autoprefixer": "^10.4.12",
        "babel-plugin-module-resolver": "^4.1.0",
        "chalk": "^4.1.2",
        "chokidar": "^3.6.0",
        "cross-env": "^7.0.3",
        "electron": "^20.1.0",
        "electron-builder": "^23.3.3",
        "electron-builder-notarize": "^1.5.1",
        "electron-devtools-installer": "^3.2.0",
        "electron-vite": "^2.1.0",
        "esbuild": "^0.17.11",
        "esbuild-node-tsc": "^2.0.5",
        "eslint": "^8.23.1",
        "eslint-plugin-vue": "^9.5.1",
        "jest": "^29.3.1",
        "postcss": "^8.4.16",
        "rimraf": "^4.3.1",
        "tailwind-scrollbar": "^2.0.1",
        "tailwindcss": "^3.3.3",
        "tsc-alias": "^1.8.2",
        "typescript": "^4.8.2",
        "vite": "^3.0.9",
        "vite-plugin-checker": "^0.5.1",
        "vue-tsc": "^0.40.13"
    },
    "dependencies": {
        "@headlessui/vue": "^1.7.9",
        "@heroicons/vue": "^2.0.11",
        "@highlightjs/vue-plugin": "^2.1.0",
        "@jsplumb/browser-ui": "^5.12.0",
        "@jsplumb/connector-bezier": "^5.12.0",
        "@prettier/plugin-php": "^0.19.2",
        "@tiago_silva_pereira/reladb": "^2.0.9",
        "@tiago_silva_pereira/vemto-template-engine": "^4.0.2",
        "@vueform/multiselect": "^2.6.1",
        "@vueuse/core": "^10.5.0",
        "animate.css": "^4.1.1",
        "blade-formatter": "^1.37.0",
        "change-case": "^4.1.2",
        "click-outside-vue3": "^4.0.1",
        "codemirror-readonly-ranges": "^0.1.0-alpha.1",
        "compare-versions": "^6.1.0",
        "constrained-editor-plugin": "^1.3.0",
        "crypto-js": "^4.1.1",
        "diff": "^5.1.0",
        "diff2html": "^3.4.33",
        "electron-log": "^5.1.5",
        "electron-updater": "^6.1.8",
        "electron-util": "0.17.2",
        "highlight.js": "^11.6.0",
        "lodash": "^4.17.21",
        "monaco-editor": "0.38.0",
        "notyf": "^3.10.0",
        "pad": "^3.2.0",
        "pinia": "^2.0.22",
        "pluralize": "^8.0.0",
        "prettier": "^2.7.1",
        "range-analyzer": "^0.1.1-alpha.2",
        "shell-path": "^3.0.0",
        "shelljs": "^0.8.5",
        "sortablejs": "^1.15.0",
        "uuid": "^9.0.0",
        "validatorjs": "^3.22.1",
        "vue": "^3.4",
        "vue-router": "^4.0.13",
        "vue3-shortkey": "^4.0.0",
        "vue3-tags-input": "^1.0.12",
        "vuedraggable": "^4.1.0",
        "wunderbaum": "^0.10.1"
    }
}
